package com.kingfisher.bookStore.book;

public class Book {
	private int id;
	private String name;
	private double price;
	private String ISBN;

	public Book() {

	}

	public Book(int id, String name, double price, String iSBN) {
		super();
		this.id = id;
		this.name = name;
		this.price = price;
		ISBN = iSBN;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}

	public String getISBN() {
		return ISBN;
	}

	public void setISBN(String iSBN) {
		ISBN = iSBN;
	}

}
